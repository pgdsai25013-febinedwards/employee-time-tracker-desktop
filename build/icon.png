<svg width="256" height="256" viewBox="0 0 256 256" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Main gradient for the fluid shape -->
    <linearGradient id="mainGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#14b8a6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#0d9488;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#10b981;stop-opacity:1" />
    </linearGradient>
    
    <!-- Glow gradient for depth -->
    <radialGradient id="glowGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#5eead4;stop-opacity:0.8" />
      <stop offset="70%" style="stop-color:#14b8a6;stop-opacity:0.4" />
      <stop offset="100%" style="stop-color:#0d9488;stop-opacity:0" />
    </radialGradient>
    
    <!-- Shadow gradient -->
    <radialGradient id="shadowGradient" cx="50%" cy="70%" r="50%">
      <stop offset="0%" style="stop-color:#000000;stop-opacity:0.2" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:0" />
    </radialGradient>
    
    <!-- Secondary layer gradient -->
    <linearGradient id="secondaryGradient" x1="0%" y1="100%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#06b6d4;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#14b8a6;stop-opacity:0.9" />
    </linearGradient>
  </defs>
  
  <!-- Background with subtle gradient -->
  <rect width="256" height="256" fill="#ffffff" opacity="0"/>
  
  <!-- Soft shadow layer -->
  <ellipse cx="128" cy="160" rx="110" ry="40" fill="url(#shadowGradient)" opacity="0.3"/>
  
  <!-- Main fluid shape - bottom layer -->
  <path d="M 128 40 C 90 40, 60 70, 60 110 C 60 140, 75 165, 100 180 C 120 192, 145 200, 170 195 C 190 191, 205 178, 210 158 C 215 135, 210 110, 195 90 C 180 70, 160 40, 128 40 Z" 
        fill="url(#mainGradient)" 
        opacity="0.85"
        filter="blur(1px)"/>
  
  <!-- Middle layer - offset for depth -->
  <path d="M 128 50 C 100 50, 75 75, 75 105 C 75 130, 88 150, 108 162 C 125 172, 145 178, 165 174 C 182 171, 195 160, 199 143 C 203 123, 199 103, 187 88 C 175 73, 158 50, 128 50 Z" 
        fill="url(#secondaryGradient)" 
        opacity="0.7"/>
  
  <!-- Top fluid shape - main focus -->
  <path d="M 128 60 C 108 60, 90 78, 90 100 C 90 120, 100 136, 116 146 C 130 154, 148 159, 165 156 C 178 154, 189 145, 192 131 C 195 115, 192 99, 182 87 C 172 75, 156 60, 128 60 Z" 
        fill="url(#mainGradient)"/>
  
  <!-- Glow effect in center -->
  <ellipse cx="128" cy="105" rx="45" ry="40" fill="url(#glowGradient)" opacity="0.6"/>
  
  <!-- Abstract timer element - minimal curved line -->
  <path d="M 128 90 L 128 110 Q 128 115, 132 117 L 145 125" 
        stroke="#ffffff" 
        stroke-width="5" 
        stroke-linecap="round" 
        stroke-linejoin="round" 
        fill="none"
        opacity="0.9"/>
  
  <!-- Small accent dots for visual interest -->
  <circle cx="165" cy="95" r="3" fill="#5eead4" opacity="0.8"/>
  <circle cx="95" cy="125" r="2.5" fill="#5eead4" opacity="0.6"/>
  
  <!-- Bottom decorative element - progress bar style -->
  <rect x="85" y="170" width="86" height="8" rx="4" ry="4" fill="url(#mainGradient)" opacity="0.6"/>
  <rect x="85" y="170" width="50" height="8" rx="4" ry="4" fill="url(#glowGradient)"/>
</svg>
